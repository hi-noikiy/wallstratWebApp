{
  "name": "Wallstrat",
  "version": "0.0.0",
  "author": "Alok Kumar Diwakar",
  "description": "Wallstrat project",
  "scripts": {
    "ng": "ng",
    "build": "ng build --prod",
    "start": "node dist/server/server.js",
    "predev": "tsc -p server",
    "dev": "concurrently \"mongod\" \"ng serve --proxy-config proxy.conf.json  --open --port 80 --host 0.0.0.0\" \"tsc -w -p server\" \"nodemon dist/server/server.js\"",
    "prod": "concurrently \"ng build --prod --build-optimizer && tsc -p server && tsc -p client && nodemon dist/server/server.js\"",
    "test": "ng test",
    "testbe": "tsc -p server && mocha dist/server/test --exit",
    "lint": "ng lint",
    "lintbe": "tslint server/**/**.ts{,x}",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.5",
    "@angular/cdk": "^6.2.1",
    "@angular/common": "^6.0.5",
    "@angular/compiler": "^6.0.5",
    "@angular/core": "^6.0.5",
    "@angular/forms": "^6.0.5",
    "@angular/http": "^6.0.5",
    "@angular/material": "^6.2.1",
    "@angular/platform-browser": "^6.0.5",
    "@angular/platform-browser-dynamic": "^6.0.5",
    "@angular/router": "^6.0.5",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.1.1",
    "chart.js": "^2.7.2",
    "core-js": "^2.5.7",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "moment": "^2.22.2",
    "mongodb": "^3.0.10",
    "mongoose": "^5.1.5",
    "morgan": "^1.9.0",
    "ng2-charts": "^1.6.0",
    "popper.js": "^1.14.3",
    "request": "^2.87.0",
    "request-promise": "^4.2.2",
    "rxjs": "^6.2.1",
    "rxjs-compat": "^6.2.1",
    "tether": "^1.4.4",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.6.8",
    "@angular/cli": "^6.0.8",
    "@angular/compiler-cli": "^6.0.5",
    "@angular/language-service": "^6.0.5",
    "@types/express": "^4.16.0",
    "@types/jasmine": "^2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "codelyzer": "~4.2.1",
    "concurrently": "^3.5.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.5",
    "protractor": "~5.3.0",
    "ts-node": "^5.0.1",
    "tslint": "~5.9.1",
    "typescript": "^2.7.2",
    "webpack": "^4.12.0"
  }
